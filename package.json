    "express": "^4.17.1",
    "foobar": "1.1.0",
    "google-auth-library": "^5.9.1",
    "pg": "7.18.1"
  },
  "devDependencies": {
    "concurrently": "5.1.0",
    "nodemon": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mehedirana/reactChatBot.git"
  },
