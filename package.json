{
  "name": "chatbot",
  "version": "1.0.0",
  "description": "React bot for webpage",
  "main": "index.js",
  "cacheDirectories": [
    "client/node_modules",
    "server/node_modules"
  ],
  "engines": {
    "node": "12.6.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "node index.js",
